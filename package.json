        "svelte": "^5.45.6",
        "svelte-check": "^4.3.4",
        "tailwindcss": "^4.1.17",
        "tiny-invariant": "^1.3.3",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.1",
        "vite": "^7.2.7",
        "vite-plugin-devtools-json": "^1.0.0",
        "wrangler": "^4.53.0"
