        "@fontsource/figtree": "^5.2.8",
        "@fontsource/geist-mono": "^5.2.6",
        "@fontsource/inter": "^5.2.6",
        "@fontsource/inter-tight": "^5.2.6",
        "@icons-pack/svelte-simple-icons": "^6.4.0",
        "@neoconfetti/svelte": "^2.2.2",
        "@sveltejs/adapter-cloudflare": "^7.1.1",
        "@sveltejs/kit": "^2.26.1",
        "@sveltejs/vite-plugin-svelte": "^6.1.0",
        "@tailwindcss/postcss": "^4.1.11",
        "@zag-js/clipboard": "^1.21.0",
        "@zag-js/svelte": "^1.21.0",
