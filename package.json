        "lucide-svelte": "^0.534.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "sanitize-html": "^2.17.0",
        "shiki": "^3.9.1",
        "svelte": "^5.37.1",
        "svelte-check": "^4.3.0",
        "tailwindcss": "^4.1.11",
        "tiny-invariant": "^1.3.3",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.38.0",
